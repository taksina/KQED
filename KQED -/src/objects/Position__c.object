        <linkType>url</linkType>
        <masterLabel>Submit for Approval</masterLabel>
        <openType>sidebar</openType>
        <protected>false</protected>
        <url>/apex/approvalconfirmation?posid={!Position__c.Id}</url>
    </webLinks>
