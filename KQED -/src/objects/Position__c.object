        <errorMessage>Close Date must be specified when Status is set to &apos;Closed.&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Min_Pay_Rule</fullName>
        <active>true</active>
        <description>Min Pay should never exceed Max Pay.</description>
        <errorConditionFormula>Min_Pay__c  &gt;  Max_Pay__c</errorConditionFormula>
        <errorDisplayField>Min_Pay__c</errorDisplayField>
        <errorMessage>Min Pay cannot exceed Max Pay.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Publish_only_when_approved</fullName>
        <active>true</active>
        <errorConditionFormula>And( Active__c, Not( ISPICKVAL( Status__c , &apos;Open - Approved&apos;)))</errorConditionFormula>
        <errorDisplayField>Active__c</errorDisplayField>
        <errorMessage>Only Open - Approved Job Requisitions can be Published to the web.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Strong_Internal_Candidate_check</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
NOT( NOT( Strong_Internal_Candidate__c ) ) , 
OR( 
ISBLANK( Internal_Candidate_Name__c )
))</errorConditionFormula>
        <errorMessage>Internal Candidate Name and Internal Candidate Id are required fields if Strong Internal Candidate is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Union_Minimum_Salary_Rule</fullName>
        <active>true</active>
        <description>Union Minimum Salary should never exceed Union Maximum Salary.</description>
        <errorConditionFormula>Union_Minimum_Salary__c  &gt; Union_Maximum_Salary__c</errorConditionFormula>
        <errorDisplayField>Union_Minimum_Salary__c</errorDisplayField>
        <errorMessage>Union Minimum Salary cannot exceed Union Maximum Salary.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Union_Salary_fields_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
OR (
ISPICKVAL(Affiliation__c, &quot;AEEF&quot;),
ISPICKVAL(Affiliation__c, &quot;NABET&quot;)),
OR(
ISBLANK(Union_Maximum_Salary__c),
ISBLANK(Union_Minimum_Salary__c))
)</errorConditionFormula>
        <errorMessage>Union Maximum and Union Minimum salary fields are required for the Union Positions(AEEF &amp; NABET)</errorMessage>
    </validationRules>
