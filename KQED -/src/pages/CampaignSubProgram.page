    function singleSelect(chk,field){
        var hdnSelectedFields = document.getElementById("j_id0:subProgramMain:hdnSelectedFields");
        if(hdnSelectedFields.value.length>0){
            if(hdnSelectedFields.value.indexOf("#")>0){
                if(chk.checked==true){
                    hdnSelectedFields.value += "#" + field;
                }
                else{
                    var arrFields = hdnSelectedFields.value.split("#");
                    var i;
                    var strFields='';
                    for(i=0; i<arrFields.length; i++){
                        if(arrFields[i] != field)
                            if(strFields.length==0)
                                strFields = arrFields[i];
                            else
                                strFields += "#" + arrFields[i];
                    }
                    hdnSelectedFields.value = strFields;
                }
            }
            else{
                if(chk.checked==true)
                    hdnSelectedFields.value += "#" + field;
                else
                    hdnSelectedFields.value = "ALL";
            }
        }
        else{
            if(chk.checked==true)
                hdnSelectedFields.value = field;
        }
        //alert(hdnSelectedFields.value);
    }
    </script>
</apex:page>