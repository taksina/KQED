    function createProgram(){
        var txtSubPrograms = document.getElementById("j_id0:subProgramMain:j_id29:j_id30:txtSubPrograms");
        //alert(txtSubPrograms.value)
        
        if(txtSubPrograms.value.length==0 || txtSubPrograms.value <=0){
            alert ("Enter number of sub program to create");
            txtSubPrograms.focus();
            return false
        }
        else if(document.getElementById("j_id0:subProgramMain:hdnSelectedFields").value.length==0){
            alert ("Select atleast one option");
            return false;
        }
        else if(txtSubPrograms.value > 12){
            alert ("You can not create more than 6 sub-program.");
            txtSubPrograms.focus();
            return false
        }
        else        
            return true;
    }
    
   
